<Window x:Class="FirstCommand.View.LevelWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:FirstCommand.View"
             mc:Ignorable="d"
             Title="Combobox Window"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <StackPanel Height="30" HorizontalAlignment="Center" Orientation="Horizontal">
            <Label Content="List Level: "></Label>
            <ComboBox Width="150" x:Name="comboBoxLevels"></ComboBox>
            <Button Content="Choose Level" Margin="15,0,0,0" Click="Button_Click"></Button>
        </StackPanel>
    </Grid>
</Window>