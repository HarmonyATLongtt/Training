<Window x:Class="FirstCommand.View.ViewComboboxWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:FirstCommand.View"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Height="30" HorizontalAlignment="Center" Orientation="Horizontal">
        <Label Content="List View: "></Label>
        <ComboBox Width="200" x:Name="comboBoxViews"></ComboBox>
        <Button Content="Choose View" Margin="15,0,0,0" Click="Button_Click"></Button>
    </StackPanel>
</Window>